            ACTIONS_aWindow_footerBtnAlign: "center",
            ACTIONS_aWindow_btnMode: "btn--default-light",
            ACTIONS_aWindow_btnHref: "window.location.href='/pages/home'",
        // THEME
            THEME_windowContainerFill: colorVars.FILLS.mainFill,
            THEME_windowTitleColor: colorVars.COLORS.main,
            THEME_windowCopyColor: colorVars.COLORS.mainMuted,
            THEME_windowBodyFirstFill: colorVars.FILLS.accentPrimaryFill,
            THEME_windowBodyFirstColor: colorVars.COLORS.accentPrimary,
            THEME_windowBodySecondFill: colorVars.FILLS.accentSecondaryFill,
            THEME_windowBodySecondColor: colorVars.COLORS.accentSecondary,
            THEME_windowFooterFill: colorVars.FILLS.accentPrimaryFill,
            THEME_windowFooterColor: colorVars.COLORS.accentSecondary,

        // TEST

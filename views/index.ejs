<%- include(moduleLocationVARS.A_PANE, {
    // GLOBAL Mode Class
        THEME_globalMode: globalModeClass,
    // MODULE Theme Class
        THEME_moduleTheme: colorVars.THEMECLASS.defaultThemeClass,

    // ALL Modules

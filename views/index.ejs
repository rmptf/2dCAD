            testColor__hl5: colorVars.COLORS.accentPrimary,
            testFill__hl5: colorVars.FILLS.hl50,

            testBtnText__hl6: "HIGHLIGHT 6",
            testColor__hl6: colorVars.COLORS.accentPrimary,
            testFill__hl6: colorVars.FILLS.hl60,

            testBtnText__hl7: "HIGHLIGHT 7",
            testColor__hl7: colorVars.COLORS.accentPrimary,
            testFill__hl7: colorVars.FILLS.hl70,

            testBtnText__hl8: "HIGHLIGHT 8",
            testColor__hl8: colorVars.COLORS.accentPrimary,
            testFill__hl8: colorVars.FILLS.hl80,

            testBtnText__hl9: "HIGHLIGHT 9",
            testColor__hl9: colorVars.COLORS.accentPrimary,
            testFill__hl9: colorVars.FILLS.hl90,

        // ALERTS
            testBtnClass__aBtn: btnVars.BTNCLASS.aButton,

            testBtnType__default: btnVars.BTNTYPES.default,

            testBtnSize__small: btnVars.BTNSIZES.small,
            testBtnSize__medium: btnVars.BTNSIZES.medium,

            testBtnStatus__primary: btnVars.BTNSTATUS.primary,
            testBtnStatus__secondary: btnVars.BTNSTATUS.secondary,
            testBtnStatus__disabled: btnVars.BTNSTATUS.disabled,
            testBtnStatus__success: btnVars.BTNSTATUS.success,
            testBtnStatus__danger: btnVars.BTNSTATUS.danger,
            testBtnStatus__warning: btnVars.BTNSTATUS.warning,
            testBtnStatus__info: btnVars.BTNSTATUS.info,

            testBtnStyle_contained: btnVars.BTNSTYLE.contained,
            testBtnStyle_outlined: btnVars.BTNSTYLE.outlined,
            testBtnStyle_text: btnVars.BTNSTYLE.text,

            testBtnText__primary: "PRIMARY",
            testBtnText__secondary: "SECONDARY",
            testBtnText__disabled: "DISABLED",
            testBtnText__success: "SUCCESS",
            testBtnText__danger: "DANGER",
            testBtnText__warning: "WARN",
            testBtnText__info: "INFO",


<div class="a-window <%= THEME_moduleTheme %> <%= THEME_windowContainerFill %>">
    <div class="a-window__header">
        <div class="a-window__header-container">
            <div class="a-window__title-container">
                <div class="a-window__title">
                    <h2 class="heading_2 bold--medium <%= THEME_windowTitleColor %>">
                        <%= CONTENT_aWindow_content.HEADER.heading %>
                    </h2>
                </div>
            </div>
            <div class="a-window__copy-container">
                <div class="a-window__copy">
                    <p class="heading_3 <%= THEME_windowCopyColor %>">
                        <%= CONTENT_aWindow_content.HEADER.paragraph %>
                    </p>
                </div>
            </div>
        </div>
    </div>
    <div class="a-window__body">
        <div class="a-window__body-container">
            <div class="a-window__copy">
                <p class="heading_3 <%= THEME_windowBodyFirstFill %> <%= THEME_windowBodyFirstColor %>">
                    <%= CONTENT_aWindow_content.BODY.paragraphFirst %>
                </p>
            </div>
            <div class="a-window__copy">
                <p class="heading_3 <%= THEME_windowBodySecondFill %> <%= THEME_windowBodySecondColor %>">
                    <%= CONTENT_aWindow_content.BODY.paragraphSecond %>
                </p>
            </div>
            <div class="a-window__slot">
                <% if (MODULE_aWindow_first) { %>
                    <%- include(MODULE_aWindow_first, {}) %>
                <% } %>
            </div>
        </div>
    </div>
    <div class="a-window__footer">
        <div class="a-window__footer-container <%= THEME_windowFooterFill %> <%= THEME_windowFooterColor %>">
            <div class="a-window__actions">
                <div class="btn-container--row btn-align--<%= ACTIONS_aWindow_footerBtnAlign %>">
                    <% if (ACTIONS_aWindow_footerBtnExist === true) { %>
                        <button class="a-button a-button__a-btn--default a-btn__default--small button--primary button--contained <%= ACTIONS_aWindow_btnMode %>" type="button" onclick=<%= ACTIONS_aWindow_btnHref %>><%= ACTIONS_aWindow_footerBtnText %></button>
                    <% } %>
                    <div class="a-window__slot">
                        <% if (MODULE_aWindow_second && ACTIONS_bForm_exist === true) { %>
                            <%- include(MODULE_aWindow_second, {}) %>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="a-window__footer">
        <div class="a-window__footer-container <%#= THEME_windowFooterFill %> <%#= THEME_windowFooterColor %>">
            <div class="a-window__actions">
                <!-- <div class="btn-container--row"> -->
                <div class="">
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl1 %> <%= testFill__hl1 %>" type="button"><%= testBtnText__hl1 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl2 %> <%= testFill__hl2 %>" type="button"><%= testBtnText__hl2 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl3 %> <%= testFill__hl3 %>" type="button"><%= testBtnText__hl3 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl4 %> <%= testFill__hl4 %>" type="button"><%= testBtnText__hl4 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl5 %> <%= testFill__hl5 %>" type="button"><%= testBtnText__hl5 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl6 %> <%= testFill__hl6 %>" type="button"><%= testBtnText__hl6 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl7 %> <%= testFill__hl7 %>" type="button"><%= testBtnText__hl7 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl8 %> <%= testFill__hl8 %>" type="button"><%= testBtnText__hl8 %></div>
                    <div class="a-button a-button__a-btn--default a-btn__default--small <%= testColor__hl9 %> <%= testFill__hl9 %>" type="button"><%= testBtnText__hl9 %></div>
                </div>
            </div>
        </div>
    </div>

    <div class="a-window__footer">
        <div class="a-window__footer-container <%#= THEME_windowFooterFill %> <%#= THEME_windowFooterColor %>">
            <div class="a-window__actions">
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--primary button--contained" type="button"><%= testBtnText__prim %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--primary button--outlined" type="button"><%= testBtnText__prim %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--secondary button--contained" type="button"><%= testBtnText__secon %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--secondary button--outlined" type="button"><%= testBtnText__secon %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--disabled button--contained" type="button"><%= testBtnText__disa %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--disabled button--outlined" type="button"><%= testBtnText__disa %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--success button--contained" type="button"><%= testBtnText__succ %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--success button--outlined" type="button"><%= testBtnText__succ %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--danger button--contained" type="button"><%= testBtnText__dang %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--danger button--outlined" type="button"><%= testBtnText__dang %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--warning button--contained" type="button"><%= testBtnText__warn %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--warning button--outlined" type="button"><%= testBtnText__warn %></button>
                </div>
                <div class="btn-container--row btn-align--center">
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--info button--contained" type="button"><%= testBtnText__info %></button>
                    <button class="a-button a-button__a-btn--default a-btn__default--small button--info button--outlined" type="button"><%= testBtnText__info %></button>
                </div>
            </div>
        </div>
    </div>
</div>


